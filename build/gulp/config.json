{
  "name": "lra",
  "paths": {
    "app": "../../theme/",
    "base": "../../src/lra.sitetheme/lra/sitetheme/resources",
    "dev": "../../theme/_site/",
    "dist": "../../src/lra.sitetheme/lra/sitetheme/resources/",
    "diazoPrefix": "/++theme++lra.sitetheme/",
    "src": "node_modules/",
    "theme": "../../theme/"
  },
  "favicon": {
    "dataFile": "faviconData.json",
    "masterPicture": "favicon.png",
    "iconPath": "assets/ico/",
    "ios": "#ffffff",
    "windows": "#2d89ef",
    "androidBackground": "#ffffff",
    "androidColor": "#ffffff",
    "safariColor": "#5bbad5",
    "revisionKey": "iet7OjeChoz1",
    "html": "_includes/base/head.html",
    "htmlDist": "_includes/base/"
  },
  "scripts": {
    "app": [
        "scripts/main.js"
        ],
    "src": [
      "fontfaceobserver/fontfaceobserver.js",
      "bootstrap-without-jquery/bootstrap3/bootstrap-without-jquery.js",
      "lazysizes/lazysizes.js",
      "flickity/dist/flickity.pkgd.js"
    ],
    "base": [
      "jquery/dist/jquery.js",
      "modernizr/modernizr.js",
      "tether/dist/js/tether.min.js",
      "bootstrap/dist/js/bootstrap.js",
      "fontfaceobserver/fontfaceobserver.js",
      "mailcheck/src/mailcheck.js",
      "JVFloat/jvfloat.js",
      "hideShowPassword/hideShowPassword.js",
      "lazysizes/plugins/ls.parent-fit.js",
      "lazysizes/lazysizes.js",
      "respimage/respimage.js",
      "flickity/dist/flickity.pkgd.js"
    ]
  },
  "styles": {
      "src": [
          "rfs/scss/_rfs.scss",
          "sass-mq/_mq.scss"
      ]
  },
  "replacementPatterns": {
        "base": [
            {
                "match": "/../../src/lra.sitetheme/lra/sitetheme/resources/styles/styles/",
                "replacement": "/styles/"
            },
            {
                "match": "/dist/styles/",
                "replacement": "/styles/"
            }
        ],
        "server": [
            {
                "match": "../../assets/",
                "replacement": "../assets/"
            },
            {
                "match": "../../dist/styles/",
                "replacement": "/styles/"
            },
            {
                "match": "../dist/styles/",
                "replacement": "/styles/"
            },
            {
                "match": "../../dist/scripts/",
                "replacement": "/scripts/"
            },
            {
                "match": "../dist/scripts/",
                "replacement": "/scripts/"
            }
        ],
        "pat": [
            {
                "match": "../assets/",
                "replacement": "assets/"
            },
            {
                "match": "assets/",
                "replacement": "/++theme++hfph.sitetheme//dist/assets/"
            },
            {
                "match": "../styles/",
                "replacement": "/++theme++hfph.sitetheme/dist/styles/"
            },
            {
                "match": "styles/",
                "replacement": "/++theme++hfph.sitetheme/dist/styles/"
            },
            {
                "match": "/styles/",
                "replacement": "/++theme++hfph.sitetheme/dist/styles/"
            },
            {
                "match": "../scripts/",
                "replacement": "/++theme++hfph.sitetheme/scripts/"
            },
            {
                "match": "scripts/",
                "replacement": "/++theme++hfph.sitetheme/scripts/"
            }
        ]
    },
    "sprite": {
        "mode": {
            "symbol": {
                "prefix": "o-icon__",
                "example": true,
                "render": {
                    "scss": true
                }
            }
        }
    }
}
